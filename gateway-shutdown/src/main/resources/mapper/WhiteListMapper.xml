<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.pkpmcloud.dao.WhiteListDAO">
    <select id="listProjectWhiteList" parameterType="java.lang.String" resultType="java.lang.String">
        SELECT computer_name
        FROM pkpm_shutdown_whitelist
        WHERE project_id =#{projectId}
    </select>
</mapper>